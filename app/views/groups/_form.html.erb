<h3>Create new group</h3>


  <%= form_for @group do |f| %>

      <table>
        <td>Name</td>
        <td><%= f.text_field :name %></td>
        <td>Member</td>
        <td><%= f.text_field :member %></td>
        <td>Popularity</td>
        <td><%= f.text_field :popularity %></td>
        <td>Description</td>
        <td><%= f.text_field :description %></td>
        <td><%= f.submit %>
      </table>

  <% end %>
