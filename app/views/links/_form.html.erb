<h3>Create new link:</h3>

  <%= form_for @link do |f| %>
    <table>
      <td>Name:</td>
      <td><%= f.text_field :name %></td>
      <td>Popularity:</td>
      <td><%= f.text_field :popularity %></td>
      <td>Description:</td>
      <td><%= f.text_field :description %></td>
      <td>Source:</td>
      <td><%= f.text_field :source %></td>
      <td><%= f.submit %></td>
    </table>
  <% end %>
